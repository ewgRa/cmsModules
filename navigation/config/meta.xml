<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE meta []>
<meta 
	license="http://www.opensource.org/licenses/bsd-license.php BSD"
	author="Evgeniy Sokolov &lt;ewgraf@gmail.com&gt;"
	DAExtends="\ewgraCms\DatabaseRequester"
	namespace="ewgraCmsModules"
>
	<Language namespace="\ewgraCms" generate="false" />

	<HttpUrl namespace="\ewgraFramework" type="Stringable" />

	<NavigationCategory>
		<properties>
			<id />
			<alias />
		</properties>
	</NavigationCategory>

	<Navigation>
		<properties>
			<id />
			<category class="NavigationCategory" />
			<uri class="\ewgraFramework\HttpUrl" />
		</properties>
	</Navigation>
	
	<NavigationData>
		<properties>
			<id />
			<navigation class="Navigation" />
			<language class="\ewgraCms\Language" />
			<text />
		</properties>
	</NavigationData>
</meta>